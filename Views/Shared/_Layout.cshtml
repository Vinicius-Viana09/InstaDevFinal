<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Instadev</title>
    <link rel="stylesheet" href="~/css/style_alterarPERFIL.css" />
    <link rel="stylesheet" href="~/css/Estilo_Per.css">
    <link rel = "stylesheet" href="~/css/login.css">
    <link rel="stylesheet" href="~/css/cad_estilo.css">
    <link rel = "stylesheet" href="~/css/login.css">
@*     <link rel = "sortcut icon" href="~/assets/instagram-logo.png" type =.png />
 *@    <script src="https://kit.fontawesome.com/4fc272cd61.js" crossorigin="anonymous"></script>
</head>

<body>
    <header>
        <div class="logo"><img src="~/assets/Instadev.png" alt="logo"></div>

        <div class="pesquisa">
            <form action="">
                <input type="text" name="" class="pesquisa_usuarios" placeholder="Pesquisar">
            </form>
        </div>

        <div class="menu">
            <a asp-area="" asp-controller="Feed" asp-action="Index"><img src="~/assets/home.png" alt="Home"></a>
            <img src="~/assets/direct.png" alt="Direct">
            <img src="~/assets/compass 1.png" alt="Compass">
            <i class="far fa-heart"></i>
            <i class="fas fa-user-circle"></i>
        </div>
    </header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div class="color">
            <p>© 2021 Instagram do Facebook</p>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
